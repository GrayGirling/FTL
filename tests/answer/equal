> set friends_a [henry=TRUE, sophie=FALSE, antoine=TRUE, eric=TRUE, erica=FALSE]
> set friends_b [henry=TRUE, sophie=FALSE, antoine=TRUE, eric=TRUE, erica=FALSE]
> set friends_c new friends_a!
