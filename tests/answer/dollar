> set c "$a or $b"
> eval c
"\$a or \$b"
> set t "thing"
> set a "something"
> set o "other"
> set b "an$o"
> eval b
"an\$o"
> echo something
something
> echo something or another
something or another
> echo a is something
a is something
> echo b is another
b is another
> echo c is something or another
c is something or another
> echo a,b,c is something or another
a,b,c is something or another
> set fn[]:{"an$o"}
> echo another
another
> set d <1,3,9>
> echo d is <1, 3, 9>
d is <1, 3, 9>
> set e <"something","$b","something or another">
> echo e is <"something", "\$b", "something or another">
e is <"something", "\$b", "something or another">
